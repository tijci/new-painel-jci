generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
}

model anuncios {
  ID Int @id @default(autoincrement())
  Titulo String
  Corretor String
  Operacao String
  DataHora DateTime @default(now())
}

model comunicados {
  ID Int @id @default(autoincrement())
  Assunto String
  Mensagem String
  Prioridade String
  DataHora DateTime @default(now())
}